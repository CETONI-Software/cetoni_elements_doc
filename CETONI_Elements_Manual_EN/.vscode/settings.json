{
    "python.defaultInterpreterPath": "${workspaceFolder}/../venv/Scripts/python.exe",
    "esbonio.sphinx.confDir": "${workspaceFolder}",
    "esbonio.server.enabled": true,
    "cSpell.words": [
        "CETONI",
        "guilabel",
        "guinum",
        "inlineimg",
        "menuselection",
        "Nemesys",
        "Qmix"
    ],
    "cSpell.enabled": true,
    "cSpell.language": "en",
    "cSpell.ignoreWords": [
        "duplica",
        "editfun",
        "guinums"
    ],
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "restructuredtext"
    ]
}
